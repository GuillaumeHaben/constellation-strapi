{
  "kind": "collectionType",
  "collectionName": "pins",
  "info": {
    "singularName": "pin",
    "pluralName": "pins",
    "displayName": "Pin"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "rarity": {
      "type": "decimal",
      "default": 1
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "pins"
    },
    "status": {
      "type": "enumeration",
      "default": "pending",
      "enum": [
        "pending",
        "approved"
      ]
    },
    "suggestedBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
